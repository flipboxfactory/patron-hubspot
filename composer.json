{
  "name": "flipboxfactory/patron-hubspot",
  "description": "A HubSpot OAuth2 provider for Patron and Craft CMS",
  "version": "1.0.0",
  "type": "craft-plugin",
  "license": "MIT",
  "keywords": [
    "cms",
    "craftcms",
    "hubspot",
    "yii2"
  ],
  "require": {
    "flipboxdigital/oauth2-hubspot": "^1.0",
    "flipboxfactory/patron": "^2.0",
    "flipboxfactory/craft-hubspot": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "codeception/codeception": "^2.4",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\patron\\hubspot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\patron\\hubspot\\tests\\": "tests/src/"
    }
  },
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/patron-hubspot/issues?state=open",
    "source": "https://github.com/flipboxfactory/patron-hubspot",
    "docs": "https://github.com/flipboxfactory/patron-hubspot",
    "rss": "https://github.com/flipboxfactory/patron-hubspot/commits/master.atom"
  },
  "extra": {
    "name": "Patron: HubSpot OAuth2 Provider",
    "handle": "patron-hubspot",
    "class": "flipbox\\patron\\hubspot\\Patron",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/patron-hubspot/master/CHANGELOG.md",
    "documentationUrl": "https://github.com/flipboxfactory/patron-hubspot/blob/master/README.md",
    "schemaVersion": "1.0.0",
    "branch-alias": {
      "dev-master": "1.0.x-dev",
      "dev-develop": "1.0.x-dev"
    }
  }
}